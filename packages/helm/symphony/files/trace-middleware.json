{
    "type": "middleware.http.tracing",
    "properties": {
      "serviceName": "symphony-api",
      "pipelines": [
        {{- if .Values.OTLP_HTTP_TRACE_ENDPOINT }}
        {
          "exporter": {
            "type": "tracing.exporters.otlpgrpc",
            "collectorUrl": "{{ tpl .Values.OTLP_HTTP_TRACE_ENDPOINT $ }}"
          }
        }
        {{- end }}
      ]
    }
  }